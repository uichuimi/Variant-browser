{
  "name": "variant-browser",
  "projectType": "application",
  "generators": {},
  "sourceRoot": "apps/variant-browser/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "dist/apps/variant-browser",
        "index": "apps/variant-browser/src/index.html",
        "main": "apps/variant-browser/src/main.ts",
        "polyfills": "apps/variant-browser/src/polyfills.ts",
        "tsConfig": "apps/variant-browser/tsconfig.app.json",
        "aot": true,
        "assets": [
          "apps/variant-browser/src/favicon.ico",
          "apps/variant-browser/src/assets"
        ],
        "styles": [
          "node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
          "node_modules/primeng/resources/themes/md-light-deeppurple/theme.css",
          "node_modules/primeng/resources/primeng.min.css",
          "node_modules/primeflex/primeflex.css",
          "node_modules/primeicons/primeicons.css",
          "node_modules/materialize-css/dist/css/materialize.min.css",
          "apps/variant-browser/src/styles.css"
        ],
        "scripts": [
          "node_modules/materialize-css/dist/js/materialize.min.js"
        ],
        "allowedCommonJsDependencies": [
          "axios"
        ]
      },
      "configurations": {
        "development": {
          "fileReplacements": [
            {
              "replace": "apps/variant-browser/src/environments/environment.ts",
              "with": "apps/variant-browser/src/environments/environment.dev.ts"
            }
          ],
          "sourceMap": true,
          "optimization": false
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/variant-browser/src/environments/environment.ts",
              "with": "apps/variant-browser/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "extractCss": true,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": true,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "6kb",
              "maximumError": "10kb"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "options": {
        "browserTarget": "variant-browser:build"
      },
      "configurations": {
        "development": {
          "browserTarget": "variant-browser:build:development"
        },
        "production": {
          "browserTarget": "variant-browser:build:production",
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "extractCss": true,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": true,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "6kb",
              "maximumError": "10kb"
            }
          ]
        }
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "variant-browser:build"
      }
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "main": "apps/variant-browser/src/test.ts",
        "polyfills": "apps/variant-browser/src/polyfills.ts",
        "tsConfig": "apps/variant-browser/tsconfig.spec.json",
        "karmaConfig": "apps/variant-browser/karma.conf.js",
        "assets": [
          "apps/variant-browser/src/favicon.ico",
          "apps/variant-browser/src/assets"
        ],
        "styles": [
          "@angular/material/prebuilt-themes/deeppurple-amber.css",
          "apps/variant-browser/src/styles.css"
        ],
        "scripts": []
      },
      "configurations": {
        "development": {
          "fileReplacements": [
            {
              "replace": "src/environments/environment.ts",
              "with": "src/environments/environment.dev.ts"
            }
          ],
          "sourceMap": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "src/environments/environment.ts",
              "with": "src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "extractCss": true,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": true,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "6kb",
              "maximumError": "10kb"
            }
          ]
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/variant-browser/**/*.ts",
          "apps/variant-browser/**/*.html"
        ]
      }
    }
  }
}
